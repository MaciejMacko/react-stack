{
  "plugins": [
    ["babel-plugin-lodash"],
    ["babel-plugin-transform-decorators-legacy"],
    ["babel-plugin-transform-react-jsx"]
  ],
  "env": {
    "commonjs": {
      "presets": [
        ["babel-preset-env", {
          "loose": true,
          "modules": "commonjs",
          "targets": {
            "browsers": ["last 2 versions", ">5%"]
          }
        }]
      ]
    },
    "es": {
      "presets": [
        ["babel-preset-env", {
          "loose": true,
          "modules": false,
          "targets": {
            "browsers": ["last 2 versions", ">5%"]
          }
        }]
      ]
    },
    "node": {
      "presets": [
        ["babel-preset-env", {
          "loose": true,
          "modules": false,
          "targets": {
            "node": 6
          }
        }]
      ]
    }
  }
}